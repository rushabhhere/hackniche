generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Agent {
  id            String          @id @default(cuid())
  name          String          @db.VarChar(255)
  ip            String          @db.VarChar(255)
  port          Int
  ResourceUsage ResourceUsage[]
}

model ResourceUsage {
  id          String @id @default(cuid())
  agentId     String
  containerId String
  cpu         Float
  ram         Float
  Agent       Agent  @relation(fields: [agentId], references: [id])
}
